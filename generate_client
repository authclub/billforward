#!/bin/bash

swagger generate client -f swagger.json -A BillForward -c client -a client \
    -O getAllPayments \
    -O getPaymentByID \
    -O getPaymentByInvoiceID \
    -O getMetadataForAccount \
    -O getMetadataForSubscription \
    -O getMetadataForRatePlan \
    -O getMetadataForProduct \
    -O setMetadataForAccount \
    -O setMetadataForSubscription \
    -O setMetadataForRatePlan \
    -O setMetadataForProduct \
    -O upsertMetadataForAccount \
    -O upsertMetadataForSubscription \
    -O upsertMetadataForRatePlan \
    -O upsertMetadataForProduct \
    -O deleteMetadataForAccount \
    -O deleteMetadataForSubscription \
    -O deleteMetadataForRatePlan \
    -O deleteMetadataForProduct \
    -O deleteAccount \
    -O getInvoiceByID \
    -O getInvoiceAsPDF \
    -O getInvoicesByAccountID \
    -O getProductRatePlanByID \
    -O getRatePlanByProductAndRatePlan \
    -O getRatePlanByProduct \
    -O getAllRatePlans \
    -O getAllProducts \
    -O cancelSubscription \
    -O createAddress \
    -O updateAddress \
    -O updateProfile \
    -O getProfile \
    -O getAllAccounts \
    -O createAccount \
    -O getAccountByID \
    -O createSubscription \
    -O getSubscriptionByAccountID \
    -O createAggregatingSubscription \
    -O batchCreateSubscriptions  \
    -M BFError \
    -M AccountPagedMetadata \
    -M Account \
    -M BillingEntityBase \
    -M PaymentMethod \
    -M InsertableBillingEntity \
    -M Profile \
    -M Address \
    -M SubscriptionPagedMetadata \
    -M Subscription \
    -M CreateSubscriptionRequest  \
    -M FixedTerm \
    -M PaymentMethodSubscriptionLink \
    -M DurationFieldType \
    -M MutableBillingEntity \
    -M PendingComponentValueChange \
    -M ProductRatePlan \
    -M Product \
    -M AggregatingComponent \
    -M PricingComponent \
    -M PricingComponentTier \
    -M PricingComponentValue \
    -M flatPricingComponent \
    -M tieredPricingComponent \
    -M tieredVolumePricingComponent \
    -M CreateSubscriptionBatchRequest \
    -M CreateAggregatingComponentRequest \
    -M CreateAggregatingSubscriptionRequest \
    -M CreateAccountRequest \
    -M CreateAddressRequest \
    -M UpdateAddressRequest \
    -M UpdateProfileRequest \
    -M PaymentMethodPagedMetadata \
    -M AuthCaptureRequest \
    -M StripeAuthCaptureRequest \
    -M ProfilePagedMetadata \
    -M AddressPagedMetadata \
    -M ProductPagedMetadata \
    -M SubscriptionCancellationPagedMetadata \
    -M ProductRatePlanPagedMetadata \
    -M InvoicePagedMetadata \
    -M Invoice \
